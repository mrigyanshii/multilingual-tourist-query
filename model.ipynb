    "\n",
    "import os\n",
    "import json\n",
    "import pandas as pd\n",
    "\n",
    "def load_massive_jsonl(root_dir):\n",
    "    merged = []\n",
    "\n",
    "    for filename in os.listdir(root_dir):\n",
    "        if filename.endswith(\".jsonl\"):\n",
    "            file_path = os.path.join(root_dir, filename)\n",
    "\n",
    "            with open(file_path, \"r\", encoding=\"utf-8\") as f:\n",
    "                for line in f:\n",
    "                    if line.strip():  \n",
    "                        obj = json.loads(line)\n",
    "\n",
    "                        merged.append({\n",
    "                            \"utterance\": obj.get(\"utt\"),\n",
    "                            \"intent\": obj.get(\"intent\"),\n",
    "                            \"locale\": obj.get(\"locale\")\n",
    "                        })\n",
    "\n",
    "    return pd.DataFrame(merged)\n",
    "\n",
    "dataset_path = r\"C:\\Users\\Asus\\Desktop\\ztudy\\projects\\multilingual_tourist_query\\massive\"\n",
    "df = load_massive_jsonl(dataset_path)\n",
    "\n",
    "print(\"Total samples loaded:\", len(df))\n",
    "df.head()\n"
   ]
